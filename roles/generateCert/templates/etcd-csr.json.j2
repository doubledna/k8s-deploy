{
  "CN": "etcd",
  "hosts": [
    "127.0.0.1",
    {% for ip in ETCD_HOSTS %}
        {% if loop.index != loop.length %}
            "{{ ip }}",
        {% else %}
            "{{ ip }}"
        {% endif %}
    {% endfor %}
  ],
  "key": {
    "algo": "rsa",
    "size": 2048
  },
  "names": [
    {
      "C": "CN",
      "ST": "BeiJing",
      "L": "BeiJing",
      "O": "k8s",
      "OU": "dev"
    }
  ]
}
