kind: KubeProxyConfiguration
apiVersion: kubeproxy.config.k8s.io/v1alpha1
clientConnection:
  burst: 200
  kubeconfig: "/etc/kubernetes/kube-proxy.kubeconfig"
  qps: 100
bindAddress: {{ ansible_default_ipv4.address }}
healthzBindAddress: {{ ansible_default_ipv4.address }}:10256
metricsBindAddress: {{ ansible_default_ipv4.address }}:10249
enableProfiling: true
clusterCIDR: {{ FLANNEL_SUBNETS }}
hostnameOverride: {{ inventory_hostname }}
mode: "ipvs"
portRange: ""
kubeProxyIPTablesConfiguration:
  masqueradeAll: false
kubeProxyIPVSConfiguration:
  scheduler: rr
  excludeCIDRs: []
